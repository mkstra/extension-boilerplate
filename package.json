{
	"name": "together",
	"version": "0.0.2",
	"license": "Apache-2.0",
	"author": "Markus ðŸŽ®",
	"scripts": {
		"prebuild": "run-p clean",
		"build": "rollup --no-sourcemap -c",
		"autobuild": "rollup -c -w",
		"prestart": "run-p clean",
		"superstart": "run-p serve:dev autobuild build",
		"start": "run-p serve:dev autobuild",
		"serve:dev": "sirv public --dev",
		"serve:prod": "sirv public",
		"clean": "rm -f public/js/bundle.js public/js/bundle.js.map public/css/bundle.css public/css/bundle.css.map",
		"eslint": "eslint --ext .js,.svelte .",
		"test": "jest --runInBand --no-cache --config ./jest.config.js"
	},
	"engines": {
		"node": ">=6.0.0",
		"npm": ">= 3.0.0"
	},
	"babel": {
		"env": {
			"test": {
				"plugins": [
					"@babel/plugin-transform-modules-commonjs"
				]
			}
		}
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/plugin-transform-modules-commonjs": "^7.4.4",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^24.8.0",
		"eslint": "^5.8.0",
		"eslint-config-nau": "^14.0.0",
		"eslint-plugin-import": "^2.15.0",
		"eslint-plugin-svelte3": "^1.1.0",
		"jest": "^24.8.0",
		"jest-localstorage-mock": "^2.4.0",
		"jest-transform-svelte": "^2.0.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.17.1",
		"prettier-plugin-svelte": "^0.4.2",
		"rollup": "^1.10.1",
		"rollup-plugin-commonjs": "^9.3.4",
		"rollup-plugin-livereload": "^1.0.0",
		"rollup-plugin-node-resolve": "^4.2.3",
		"rollup-plugin-svelte": "^5.0.3",
		"rollup-plugin-terser": "^4.0.4",
		"sirv-cli": "^0.4.0",
		"stylelint": "^9.10.1",
		"stylelint-config-standard": "^18.2.0",
		"svelte": "3.5.4"
	},
	"dependencies": {
		"chrome-promise": "^3.0.5",
		"firebase": "^7.2.3",
		"firebase-tools": "^9.2.2",
		"firebaseui": "^4.7.1",
		"hotkeys-js": "^3.8.2",
		"normalize-url": "^5.3.0",
		"ramda": "^0.27.1",
		"rxfire": "^3.8.8",
		"rxjs": "^6.5.3",
		"toastr": "^2.1.4"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 11"
	]
}
